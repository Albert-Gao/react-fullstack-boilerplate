{
    "name": "fullstack-react-boilerplate",
    "description": "",
    "version": "0.0.0",
    "homepage": "",
    "main": "server",
    "keywords": [
        "feathers"
    ],
    "author": {
        "name": "Albert Gao",
        "email": "albertgaohy@gmail.com"
    },
    "contributors": [],
    "bugs": {},
    "directories": {
        "lib": "server",
        "test": "tests/"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "scripts": {
        "test": "NODE_ENV=test ./node_modules/.bin/jest --config=config/packages/jest.config.json",
        "start": "node server/",
        "e2e": "./node_modules/.bin/nightwatch --config=configs/nightwatch.json",
        "dev:build": "NODE_ENV=development ./node_modules/.bin/webpack --color --progress --config=./config/packages/webpack.dev.js --display-error-details",
        "dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --progress --config ./config/packages/webpack.dev.js",
        "build": "NODE_ENV=production ./node_modules/.bin/webpack --color --progress --config ./config/packages/webpack.prod.js",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "dependencies": {
        "axios": "^0.16.2",
        "babel-runtime": "^6.26.0",
        "body-parser": "^1.18.1",
        "compression": "^1.7.0",
        "cors": "^2.8.4",
        "feathers": "^2.2.0",
        "feathers-configuration": "^0.4.1",
        "feathers-errors": "^2.9.2",
        "feathers-hooks": "^2.0.2",
        "feathers-hooks-common": "^3.7.3",
        "feathers-rest": "^1.8.0",
        "feathers-socketio": "^2.0.0",
        "helmet": "^3.8.1",
        "history": "^4.7.2",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "redux": "^3.7.2",
        "redux-saga": "^0.15.6",
        "serve-favicon": "^2.4.4",
        "winston": "^2.3.1"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.4",
        "babel-core": "^6.26.0",
        "babel-jest": "^21.0.2",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "clean-webpack-plugin": "^0.1.16",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.28.7",
        "eslint": "^4.7.1",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.3.0",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "html-webpack-plugin": "^2.30.1",
        "jest": "^21.1.0",
        "node-sass": "^4.5.3",
        "postcss-flexbugs-fixes": "^3.2.0",
        "postcss-loader": "^2.0.6",
        "prop-types": "^15.5.10",
        "react-hot-loader": "^3.0.0-beta.7",
        "request": "^2.81.0",
        "request-promise": "^4.2.1",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.18.2",
        "url-loader": "^0.5.9",
        "webpack": "^3.6.0",
        "webpack-dev-server": "^2.8.2",
        "write-file-webpack-plugin": "^4.1.0",
        "@storybook/react": "^3.2.8",
        "@storybook/addon-actions": "^3.2.6",
        "@storybook/addon-links": "^3.2.6"
    },
    "babel": {
        "env": {
            "test": {
                "presets": [
                    "env",
                    "react"
                ],
                "plugins": [
                    "transform-object-rest-spread",
                    "transform-runtime",
                    "react-hot-loader/babel"
                ]
            }
        }
    }
}
