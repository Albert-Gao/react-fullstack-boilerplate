    "webpack-dev-server": "^3.1.1",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "env",
          "react"
        ],
        "plugins": [
          "transform-object-rest-spread",
          "transform-runtime",
          "react-hot-loader/babel"
        ]
      }
    }
  }
}
