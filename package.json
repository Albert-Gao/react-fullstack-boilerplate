{
  "name": "fullstack-react-boilerplate",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "main": "server",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Albert Gao",
    "email": "albertgaohy@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server",
    "test": "tests/"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/jest --config=config/packages/jest.config.json",
    "start": "node server/",
    "e2e": "./node_modules/.bin/nightwatch --config=configs/nightwatch.json",
    "dev:build": "NODE_ENV=development ./node_modules/.bin/webpack --color --progress --config=./config/packages/webpack.dev.js --display-error-details",
    "dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --progress --config ./config/packages/webpack.dev.js",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --color --progress --config ./config/packages/webpack.prod.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.2.1",
    "@feathersjs/express": "^1.1.2",
    "@feathersjs/feathers": "^3.0.5",
    "@feathersjs/socketio": "^3.0.2",
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "feathers-hooks-common": "^4.8.0",
    "helmet": "^3.9.0",
    "history": "^4.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "serve-favicon": "^2.4.5",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.2",
    "prop-types": "^15.6.1",
    "react-hot-loader": "^4.0.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
